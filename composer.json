{
  "name": "parfaitementweb/filament-plugin-translatable-inline",
  "description": "An inline translation plugin for Filament 4.",
  "keywords": [
    "parfaitementweb",
    "laravel",
    "filament",
    "translatable",
    "spatie",
    "lara-zeus",
    "inline"
  ],
  "homepage": "https://github.com/parfaitementweb/filament-plugin-translatable-inline",
  "support": {
    "issues": "https://github.com/parfaitementweb/filament-plugin-translatable-inline",
    "source": "https://github.com/parfaitementweb/filament-plugin-translatable-inline"
  },
  "license": "MIT",
  "require": {
    "php": "^8.2",
    "filament/forms": "^4.0",
    "lara-zeus/spatie-translatable": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Parfaitementweb\\FilamentPluginTranslatableInline\\": "src/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Parfaitementweb\\FilamentPluginTranslatableInline\\FilamentPluginTranslatableInlineServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
